    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/styled-components": "^4.1.18",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@frontendmasters/pet": "^1.0.3",
